<functions>
    <!-- xpath -->
    <function>
        <name>xpath</name>
        <type>executable_pool</type>
        <command>xpath</command>
        <format>tabseparated</format>
        <argument>
            <type>String</type>
            <name>value</name>
        </argument>
        <argument>
            <type>String</type>
