        <command>xpath_to_array</command>
        <format>tabseparated</format>
        <argument>
            <type>String</type>
            <name>value</name>
        </argument>
        <argument>
            <type>String</type>
            <name>value</name>
        </argument>
        <return_type>Array(String)</return_type>
